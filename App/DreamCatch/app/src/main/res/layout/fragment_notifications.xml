<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/scrollView"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.notifications.NotificationsFragment"
    android:background="@drawable/b3">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <androidx.cardview.widget.CardView
            android:id="@+id/cardView4"
            android:layout_width="match_parent"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_height="50dp"
            android:layout_marginTop="50dp"
            app:cardBackgroundColor="#327B91BF"
            app:cardCornerRadius="20dp" >

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:textStyle="bold"
                android:text="Date: "
                android:textColor="#BCBCBC"/>

            <TextView
                android:id="@+id/textView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="100dp"
                android:layout_marginTop="10dp"
                android:text=" "
                android:textStyle="bold"
                android:textColor="#BCBCBC"/>
        </androidx.cardview.widget.CardView>


        <androidx.cardview.widget.CardView
            android:id="@+id/cardView5"
            android:layout_width="match_parent"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_height="100dp"
            android:layout_marginTop="240dp"
            app:cardBackgroundColor="#327B91BF"
            app:cardCornerRadius="20dp" >

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="20dp"
                android:text="Sleep Score"
                android:textStyle="bold"
                android:textColor="#BCBCBC"/>

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="70dp"
                android:layout_marginTop="50dp"
                android:text="80"
                android:textStyle="bold"
                android:textColor="#BCBCBC"/>

            <com.github.mikephil.charting.charts.PieChart
                android:id="@+id/donutChart"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="180dp"
                android:layout_width="80dp"
                android:layout_height="80dp"/>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView6"
            android:layout_width="match_parent"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_height="100dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="120dp"
            app:cardBackgroundColor="#327B91BF"
            app:cardCornerRadius="20dp" >

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="20dp"
                android:text="Asleep "
                android:textStyle="bold"
                android:textColor="#BCBCBC"/>

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="50dp"
                android:text="5 hours"
                android:textStyle="bold"
                android:textColor="#BCBCBC"/>

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="20dp"
                android:text="Slept at "
                android:textStyle="bold"
                android:textColor="#BCBCBC"/>

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="50dp"
                android:text="11.25pm"
                android:textStyle="bold"
                android:textColor="#BCBCBC"/>

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="250dp"
                android:layout_marginTop="20dp"
                android:textStyle="bold"
                android:text="Woke up at "
                android:textColor="#BCBCBC"/>

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="250dp"
                android:layout_marginTop="50dp"
                android:text="8.25am"
                android:textStyle="bold"
                android:textColor="#BCBCBC"/>


        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
                android:id="@+id/cardView2"
                android:layout_width="match_parent"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:layout_height="360dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="360dp"
                app:cardBackgroundColor="#327B91BF"
                app:cardCornerRadius="20dp" >



            <com.github.mikephil.charting.charts.PieChart
                android:id="@+id/pieChart"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginLeft="50dp"
                android:layout_marginTop="20dp" />


            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="65dp"
                android:layout_marginTop="150dp"
                android:text="Sleep Length"
                android:textStyle="bold"
                android:textColor="#BCBCBC"/>

            <com.github.mikephil.charting.charts.PieChart
                android:id="@+id/pieChart2"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="200dp"
                android:layout_width="100dp"
                android:layout_height="100dp"/>

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="215dp"
                android:layout_marginTop="150dp"
                android:text="REM Sleep"
                android:textStyle="bold"
                android:textColor="#BCBCBC"/>

            <com.github.mikephil.charting.charts.PieChart
                android:id="@+id/pieChart3"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginLeft="50dp"
                android:layout_marginTop="200dp" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="65dp"
                android:layout_marginTop="320dp"
                android:text="Deep Sleep"
                android:textStyle="bold"
                android:textColor="#BCBCBC"/>

            <com.github.mikephil.charting.charts.PieChart
                android:id="@+id/pieChart4"
                android:layout_marginTop="200dp"
                android:layout_marginLeft="200dp"
                android:layout_width="100dp"
                android:layout_height="100dp"/>

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="215dp"
                android:layout_marginTop="320dp"
                android:text="Light Sleep"
                android:textStyle="bold"
                android:textColor="#BCBCBC"/>
        </androidx.cardview.widget.CardView>


        <androidx.cardview.widget.CardView
                android:id="@+id/cardView3"
                android:layout_width="match_parent"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:layout_height="120dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="750dp"
                app:cardBackgroundColor="#327B91BF"
                app:cardCornerRadius="20dp" >

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Environment"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="20dp"
                android:textStyle="bold"
                android:textColor="#BCBCBC"/>

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Feedback"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="80dp"
                android:textStyle="bold"
                android:textColor="#BCBCBC"/>

            <TextView
                android:id="@+id/textView30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Temperature"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="50dp"
                android:textStyle="bold"
                android:textColor="#BCBCBC"/>

            <com.github.mikephil.charting.charts.PieChart
                android:id="@+id/donutChart2"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="180dp"
                android:layout_width="80dp"
                android:layout_height="80dp"/>
        </androidx.cardview.widget.CardView>


        <androidx.cardview.widget.CardView
            android:id="@+id/cardView10"
            android:layout_width="match_parent"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_height="300dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="890dp"
            app:cardBackgroundColor="#007B91BF"
            app:cardCornerRadius="20dp" >

        </androidx.cardview.widget.CardView>


    </FrameLayout>
</ScrollView>
